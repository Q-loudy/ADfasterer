{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\nimport \"core-js/modules/web.dom-collections.iterator.js\";\nimport CelestialQuoteHistory from \"@/components/CelestialQuoteHistory\";\nimport GlyphSetPreview from \"@/components/GlyphSetPreview\";\nimport PrimaryButton from \"@/components/PrimaryButton\";\nimport { V_REDUCTION_MODE } from \"@/core/secret-formula\";\nimport VUnlockRequirement from \"./VUnlockRequirement\";\nexport default {\n  name: \"VTab\",\n  components: {\n    CelestialQuoteHistory,\n    VUnlockRequirement,\n    PrimaryButton,\n    GlyphSetPreview\n  },\n\n  data() {\n    return {\n      mainUnlock: false,\n      canUnlockCelestial: false,\n      totalUnlocks: 0,\n      pp: 0,\n      showReduction: false,\n      runRecords: [],\n      runGlyphs: [],\n      isFlipped: false,\n      wantsFlipped: true,\n      isRunning: false,\n      hasAlchemy: false\n    };\n  },\n\n  computed: {\n    mainUnlockDB: () => GameDatabase.celestials.v.mainUnlock,\n\n    celestialUnlockClassObject() {\n      return {\n        \"o-v-milestone\": true,\n        \"o-v-milestone--unlocked\": this.canUnlockCelestial,\n        \"c-v-unlock-button--enabled\": this.canUnlockCelestial\n      };\n    },\n\n    // If V is flipped, change the layout of the grid\n    hexGrid() {\n      return this.isFlipped && this.wantsFlipped ? [VRunUnlocks.all[6], {}, {}, {}, {\n        isRunButton: true\n      }, VRunUnlocks.all[7], VRunUnlocks.all[8], {}, {}] : [VRunUnlocks.all[0], VRunUnlocks.all[1], {}, VRunUnlocks.all[5], {\n        isRunButton: true\n      }, VRunUnlocks.all[2], VRunUnlocks.all[4], VRunUnlocks.all[3], {}];\n    },\n\n    vUnlock: () => VUnlocks.vAchievementUnlock,\n\n    runMilestones() {\n      return [[VUnlocks.shardReduction, VUnlocks.adPow, VUnlocks.fastAutoEC], [VUnlocks.autoAutoClean, VUnlocks.achievementBH, VUnlocks.raUnlock]];\n    },\n\n    runButtonClassObject() {\n      return {\n        \"l-v-hexagon\": true,\n        \"c-v-run-button\": true,\n        \"c-v-run-button--running\": this.isRunning,\n        \"c-celestial-run-button--clickable\": !this.isDoomed,\n        \"o-pelle-disabled-pointer\": this.isDoomed\n      };\n    },\n\n    runDescription() {\n      return GameDatabase.celestials.descriptions[3].effects().replace(/^[0-9A-Z_a-z]/, c => c.toUpperCase());\n    },\n\n    isDoomed: () => Pelle.isDoomed\n  },\n  methods: {\n    update() {\n      this.mainUnlock = VUnlocks.vAchievementUnlock.isUnlocked;\n      this.canUnlockCelestial = V.canUnlockCelestial;\n      this.totalUnlocks = V.spaceTheorems;\n      this.pp = Currency.perkPoints.value;\n      this.showReduction = VUnlocks.shardReduction.isUnlocked;\n      this.runRecords = Array.from(player.celestials.v.runRecords);\n      this.runGlyphs = player.celestials.v.runGlyphs.map(gList => Glyphs.copyForRecords(gList));\n      this.isFlipped = V.isFlipped;\n      this.wantsFlipped = player.celestials.v.wantsFlipped;\n      this.isRunning = V.isRunning;\n      this.hasAlchemy = Ra.unlocks.unlockGlyphAlchemy.canBeApplied;\n    },\n\n    unlockCelestial() {\n      if (V.canUnlockCelestial) V.unlockCelestial();\n    },\n\n    startRun() {\n      if (this.isDoomed) return;\n      Modal.celestials.show({\n        name: \"V's\",\n        number: 3\n      });\n    },\n\n    has(info) {\n      return info.isUnlocked;\n    },\n\n    mode(hex) {\n      return hex.config.mode === V_REDUCTION_MODE.SUBTRACTION ? \"reduced\" : \"divided\";\n    },\n\n    reductionValue(hex) {\n      return hex.config.mode === V_REDUCTION_MODE.SUBTRACTION ? formatInt(hex.reduction) : format(Decimal.pow10(hex.reduction));\n    },\n\n    showRecord(hex) {\n      return this.runRecords[hex.id] > 0 || hex.completions > 0;\n    },\n\n    reduceGoals(hex) {\n      if (!Currency.perkPoints.purchase(hex.reductionCost)) return;\n      const steps = hex.config.reductionStepSize ? hex.config.reductionStepSize : 1;\n      player.celestials.v.goalReductionSteps[hex.id] += steps;\n\n      for (const unlock of VRunUnlocks.all) {\n        unlock.tryComplete();\n      }\n\n      V.checkForUnlocks();\n    },\n\n    reductionTooltip(hex) {\n      return `Spend ${quantify(\"Perk Point\", hex.reductionCost, 2, 0)}\n        to reduce goal by ${format(hex.config.perReductionStep)}`;\n    },\n\n    hexColor(hex) {\n      const completions = hex.completions;\n      const maxed = hex.config.values.length;\n      if (completions === maxed) return \"var(--color-v--base)\";\n      const r = 255 - 5 * completions;\n      const g = 255 - 10 * completions;\n      const b = 255 - 20 * completions;\n      return `rgb(${r},${g},${b})`;\n    },\n\n    toggleFlipped() {\n      player.celestials.v.wantsFlipped = !this.wantsFlipped;\n    },\n\n    createCursedGlyph() {\n      Glyphs.giveCursedGlyph();\n    }\n\n  }\n};","map":{"version":3,"sources":["VTab.vue"],"names":[],"mappings":";;;AACA,OAAA,qBAAA,MAAA,oCAAA;AACA,OAAA,eAAA,MAAA,8BAAA;AACA,OAAA,aAAA,MAAA,4BAAA;AACA,SAAA,gBAAA,QAAA,uBAAA;AACA,OAAA,kBAAA,MAAA,sBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,qBADA;AAEA,IAAA,kBAFA;AAGA,IAAA,aAHA;AAIA,IAAA;AAJA,GAFA;;AAQA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,UAAA,EAAA,KADA;AAEA,MAAA,kBAAA,EAAA,KAFA;AAGA,MAAA,YAAA,EAAA,CAHA;AAIA,MAAA,EAAA,EAAA,CAJA;AAKA,MAAA,aAAA,EAAA,KALA;AAMA,MAAA,UAAA,EAAA,EANA;AAOA,MAAA,SAAA,EAAA,EAPA;AAQA,MAAA,SAAA,EAAA,KARA;AASA,MAAA,YAAA,EAAA,IATA;AAUA,MAAA,SAAA,EAAA,KAVA;AAWA,MAAA,UAAA,EAAA;AAXA,KAAA;AAaA,GAtBA;;AAuBA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,EAAA,MAAA,YAAA,CAAA,UAAA,CAAA,CAAA,CAAA,UADA;;AAEA,IAAA,0BAAA,GAAA;AACA,aAAA;AACA,yBAAA,IADA;AAEA,mCAAA,KAAA,kBAFA;AAGA,sCAAA,KAAA;AAHA,OAAA;AAKA,KARA;;AASA;AACA,IAAA,OAAA,GAAA;AACA,aAAA,KAAA,SAAA,IAAA,KAAA,YAAA,GACA,CACA,WAAA,CAAA,GAAA,CAAA,CAAA,CADA,EAEA,EAFA,EAGA,EAHA,EAIA,EAJA,EAKA;AAAA,QAAA,WAAA,EAAA;AAAA,OALA,EAMA,WAAA,CAAA,GAAA,CAAA,CAAA,CANA,EAOA,WAAA,CAAA,GAAA,CAAA,CAAA,CAPA,EAQA,EARA,EASA,EATA,CADA,GAYA,CACA,WAAA,CAAA,GAAA,CAAA,CAAA,CADA,EAEA,WAAA,CAAA,GAAA,CAAA,CAAA,CAFA,EAGA,EAHA,EAIA,WAAA,CAAA,GAAA,CAAA,CAAA,CAJA,EAKA;AAAA,QAAA,WAAA,EAAA;AAAA,OALA,EAMA,WAAA,CAAA,GAAA,CAAA,CAAA,CANA,EAOA,WAAA,CAAA,GAAA,CAAA,CAAA,CAPA,EAQA,WAAA,CAAA,GAAA,CAAA,CAAA,CARA,EASA,EATA,CAZA;AAuBA,KAlCA;;AAmCA,IAAA,OAAA,EAAA,MAAA,QAAA,CAAA,kBAnCA;;AAoCA,IAAA,aAAA,GAAA;AACA,aAAA,CACA,CACA,QAAA,CAAA,cADA,EAEA,QAAA,CAAA,KAFA,EAGA,QAAA,CAAA,UAHA,CADA,EAMA,CACA,QAAA,CAAA,aADA,EAEA,QAAA,CAAA,aAFA,EAGA,QAAA,CAAA,QAHA,CANA,CAAA;AAYA,KAjDA;;AAkDA,IAAA,oBAAA,GAAA;AACA,aAAA;AACA,uBAAA,IADA;AAEA,0BAAA,IAFA;AAGA,mCAAA,KAAA,SAHA;AAIA,6CAAA,CAAA,KAAA,QAJA;AAKA,oCAAA,KAAA;AALA,OAAA;AAOA,KA1DA;;AA2DA,IAAA,cAAA,GAAA;AACA,aAAA,YAAA,CAAA,UAAA,CAAA,YAAA,CAAA,CAAA,EAAA,OAAA,GAAA,OAAA,CAAA,eAAA,EAAA,CAAA,IAAA,CAAA,CAAA,WAAA,EAAA,CAAA;AACA,KA7DA;;AA8DA,IAAA,QAAA,EAAA,MAAA,KAAA,CAAA;AA9DA,GAvBA;AAuFA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,GAAA;AACA,WAAA,UAAA,GAAA,QAAA,CAAA,kBAAA,CAAA,UAAA;AACA,WAAA,kBAAA,GAAA,CAAA,CAAA,kBAAA;AACA,WAAA,YAAA,GAAA,CAAA,CAAA,aAAA;AACA,WAAA,EAAA,GAAA,QAAA,CAAA,UAAA,CAAA,KAAA;AACA,WAAA,aAAA,GAAA,QAAA,CAAA,cAAA,CAAA,UAAA;AACA,WAAA,UAAA,GAAA,KAAA,CAAA,IAAA,CAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAAA,UAAA,CAAA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAAA,SAAA,CAAA,GAAA,CAAA,KAAA,IAAA,MAAA,CAAA,cAAA,CAAA,KAAA,CAAA,CAAA;AACA,WAAA,SAAA,GAAA,CAAA,CAAA,SAAA;AACA,WAAA,YAAA,GAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAAA,YAAA;AACA,WAAA,SAAA,GAAA,CAAA,CAAA,SAAA;AACA,WAAA,UAAA,GAAA,EAAA,CAAA,OAAA,CAAA,kBAAA,CAAA,YAAA;AACA,KAbA;;AAcA,IAAA,eAAA,GAAA;AACA,UAAA,CAAA,CAAA,kBAAA,EAAA,CAAA,CAAA,eAAA;AACA,KAhBA;;AAiBA,IAAA,QAAA,GAAA;AACA,UAAA,KAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAAA;AACA,KApBA;;AAqBA,IAAA,GAAA,CAAA,IAAA,EAAA;AACA,aAAA,IAAA,CAAA,UAAA;AACA,KAvBA;;AAwBA,IAAA,IAAA,CAAA,GAAA,EAAA;AACA,aAAA,GAAA,CAAA,MAAA,CAAA,IAAA,KAAA,gBAAA,CAAA,WAAA,GAAA,SAAA,GAAA,SAAA;AACA,KA1BA;;AA2BA,IAAA,cAAA,CAAA,GAAA,EAAA;AACA,aAAA,GAAA,CAAA,MAAA,CAAA,IAAA,KAAA,gBAAA,CAAA,WAAA,GACA,SAAA,CAAA,GAAA,CAAA,SAAA,CADA,GAEA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA,GAAA,CAAA,SAAA,CAAA,CAFA;AAGA,KA/BA;;AAgCA,IAAA,UAAA,CAAA,GAAA,EAAA;AACA,aAAA,KAAA,UAAA,CAAA,GAAA,CAAA,EAAA,IAAA,CAAA,IAAA,GAAA,CAAA,WAAA,GAAA,CAAA;AACA,KAlCA;;AAmCA,IAAA,WAAA,CAAA,GAAA,EAAA;AACA,UAAA,CAAA,QAAA,CAAA,UAAA,CAAA,QAAA,CAAA,GAAA,CAAA,aAAA,CAAA,EAAA;AACA,YAAA,KAAA,GAAA,GAAA,CAAA,MAAA,CAAA,iBAAA,GAAA,GAAA,CAAA,MAAA,CAAA,iBAAA,GAAA,CAAA;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAAA,kBAAA,CAAA,GAAA,CAAA,EAAA,KAAA,KAAA;;AACA,WAAA,MAAA,MAAA,IAAA,WAAA,CAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA;AACA;;AACA,MAAA,CAAA,CAAA,eAAA;AACA,KA3CA;;AA4CA,IAAA,gBAAA,CAAA,GAAA,EAAA;AACA,aAAA,SAAA,QAAA,CAAA,YAAA,EAAA,GAAA,CAAA,aAAA,EAAA,CAAA,EAAA,CAAA,CAAA;AACA,4BAAA,MAAA,CAAA,GAAA,CAAA,MAAA,CAAA,gBAAA,CAAA,EADA;AAEA,KA/CA;;AAgDA,IAAA,QAAA,CAAA,GAAA,EAAA;AACA,YAAA,WAAA,GAAA,GAAA,CAAA,WAAA;AACA,YAAA,KAAA,GAAA,GAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA;AACA,UAAA,WAAA,KAAA,KAAA,EAAA,OAAA,sBAAA;AACA,YAAA,CAAA,GAAA,MAAA,IAAA,WAAA;AACA,YAAA,CAAA,GAAA,MAAA,KAAA,WAAA;AACA,YAAA,CAAA,GAAA,MAAA,KAAA,WAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAxDA;;AAyDA,IAAA,aAAA,GAAA;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAAA,YAAA,GAAA,CAAA,KAAA,YAAA;AACA,KA3DA;;AA4DA,IAAA,iBAAA,GAAA;AACA,MAAA,MAAA,CAAA,eAAA;AACA;;AA9DA;AAvFA,CAAA","sourcesContent":["<script>\r\nimport CelestialQuoteHistory from \"@/components/CelestialQuoteHistory\";\r\nimport GlyphSetPreview from \"@/components/GlyphSetPreview\";\r\nimport PrimaryButton from \"@/components/PrimaryButton\";\r\nimport { V_REDUCTION_MODE } from \"@/core/secret-formula\";\r\nimport VUnlockRequirement from \"./VUnlockRequirement\";\r\n\r\nexport default {\r\n  name: \"VTab\",\r\n  components: {\r\n    CelestialQuoteHistory,\r\n    VUnlockRequirement,\r\n    PrimaryButton,\r\n    GlyphSetPreview\r\n  },\r\n  data() {\r\n    return {\r\n      mainUnlock: false,\r\n      canUnlockCelestial: false,\r\n      totalUnlocks: 0,\r\n      pp: 0,\r\n      showReduction: false,\r\n      runRecords: [],\r\n      runGlyphs: [],\r\n      isFlipped: false,\r\n      wantsFlipped: true,\r\n      isRunning: false,\r\n      hasAlchemy: false,\r\n    };\r\n  },\r\n  computed: {\r\n    mainUnlockDB: () => GameDatabase.celestials.v.mainUnlock,\r\n    celestialUnlockClassObject() {\r\n      return {\r\n        \"o-v-milestone\": true,\r\n        \"o-v-milestone--unlocked\": this.canUnlockCelestial,\r\n        \"c-v-unlock-button--enabled\": this.canUnlockCelestial\r\n      };\r\n    },\r\n    // If V is flipped, change the layout of the grid\r\n    hexGrid() {\r\n      return this.isFlipped && this.wantsFlipped\r\n        ? [\r\n          VRunUnlocks.all[6],\r\n          {},\r\n          {},\r\n          {},\r\n          { isRunButton: true },\r\n          VRunUnlocks.all[7],\r\n          VRunUnlocks.all[8],\r\n          {},\r\n          {}\r\n        ]\r\n        : [\r\n          VRunUnlocks.all[0],\r\n          VRunUnlocks.all[1],\r\n          {},\r\n          VRunUnlocks.all[5],\r\n          { isRunButton: true },\r\n          VRunUnlocks.all[2],\r\n          VRunUnlocks.all[4],\r\n          VRunUnlocks.all[3],\r\n          {}\r\n        ];\r\n    },\r\n    vUnlock: () => VUnlocks.vAchievementUnlock,\r\n    runMilestones() {\r\n      return [\r\n        [\r\n          VUnlocks.shardReduction,\r\n          VUnlocks.adPow,\r\n          VUnlocks.fastAutoEC\r\n        ],\r\n        [\r\n          VUnlocks.autoAutoClean,\r\n          VUnlocks.achievementBH,\r\n          VUnlocks.raUnlock\r\n        ],\r\n      ];\r\n    },\r\n    runButtonClassObject() {\r\n      return {\r\n        \"l-v-hexagon\": true,\r\n        \"c-v-run-button\": true,\r\n        \"c-v-run-button--running\": this.isRunning,\r\n        \"c-celestial-run-button--clickable\": !this.isDoomed,\r\n        \"o-pelle-disabled-pointer\": this.isDoomed\r\n      };\r\n    },\r\n    runDescription() {\r\n      return GameDatabase.celestials.descriptions[3].effects().replace(/^\\w/u, c => c.toUpperCase());\r\n    },\r\n    isDoomed: () => Pelle.isDoomed,\r\n  },\r\n  methods: {\r\n    update() {\r\n      this.mainUnlock = VUnlocks.vAchievementUnlock.isUnlocked;\r\n      this.canUnlockCelestial = V.canUnlockCelestial;\r\n      this.totalUnlocks = V.spaceTheorems;\r\n      this.pp = Currency.perkPoints.value;\r\n      this.showReduction = VUnlocks.shardReduction.isUnlocked;\r\n      this.runRecords = Array.from(player.celestials.v.runRecords);\r\n      this.runGlyphs = player.celestials.v.runGlyphs.map(gList => Glyphs.copyForRecords(gList));\r\n      this.isFlipped = V.isFlipped;\r\n      this.wantsFlipped = player.celestials.v.wantsFlipped;\r\n      this.isRunning = V.isRunning;\r\n      this.hasAlchemy = Ra.unlocks.unlockGlyphAlchemy.canBeApplied;\r\n    },\r\n    unlockCelestial() {\r\n      if (V.canUnlockCelestial) V.unlockCelestial();\r\n    },\r\n    startRun() {\r\n      if (this.isDoomed) return;\r\n      Modal.celestials.show({ name: \"V's\", number: 3 });\r\n    },\r\n    has(info) {\r\n      return info.isUnlocked;\r\n    },\r\n    mode(hex) {\r\n      return hex.config.mode === V_REDUCTION_MODE.SUBTRACTION ? \"reduced\" : \"divided\";\r\n    },\r\n    reductionValue(hex) {\r\n      return hex.config.mode === V_REDUCTION_MODE.SUBTRACTION\r\n        ? formatInt(hex.reduction)\r\n        : format(Decimal.pow10(hex.reduction));\r\n    },\r\n    showRecord(hex) {\r\n      return this.runRecords[hex.id] > 0 || hex.completions > 0;\r\n    },\r\n    reduceGoals(hex) {\r\n      if (!Currency.perkPoints.purchase(hex.reductionCost)) return;\r\n      const steps = hex.config.reductionStepSize ? hex.config.reductionStepSize : 1;\r\n      player.celestials.v.goalReductionSteps[hex.id] += steps;\r\n      for (const unlock of VRunUnlocks.all) {\r\n        unlock.tryComplete();\r\n      }\r\n      V.checkForUnlocks();\r\n    },\r\n    reductionTooltip(hex) {\r\n      return `Spend ${quantify(\"Perk Point\", hex.reductionCost, 2, 0)}\r\n        to reduce goal by ${format(hex.config.perReductionStep)}`;\r\n    },\r\n    hexColor(hex) {\r\n      const completions = hex.completions;\r\n      const maxed = hex.config.values.length;\r\n      if (completions === maxed) return \"var(--color-v--base)\";\r\n      const r = 255 - 5 * completions;\r\n      const g = 255 - 10 * completions;\r\n      const b = 255 - 20 * completions;\r\n      return `rgb(${r},${g},${b})`;\r\n    },\r\n    toggleFlipped() {\r\n      player.celestials.v.wantsFlipped = !this.wantsFlipped;\r\n    },\r\n    createCursedGlyph() {\r\n      Glyphs.giveCursedGlyph();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div class=\"l-v-celestial-tab\">\r\n    <CelestialQuoteHistory celestial=\"v\" />\r\n    <div\r\n      v-if=\"!mainUnlock\"\r\n      class=\"c-v-info-text\"\r\n    >\r\n      <v-unlock-requirement\r\n        v-for=\"req in mainUnlockDB\"\r\n        :key=\"req.name\"\r\n        :db-entry=\"req\"\r\n      />\r\n      <div class=\"l-v-milestones-grid__row\">\r\n        <div\r\n          :class=\"celestialUnlockClassObject\"\r\n          @click=\"unlockCelestial\"\r\n        >\r\n          <p>{{ vUnlock.description }}</p>\r\n          <p>{{ vUnlock.rewardText }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else>\r\n      <div\r\n        v-if=\"isFlipped\"\r\n        class=\"c-v-info-text\"\r\n      >\r\n        <PrimaryButton\r\n          class=\"o-primary-btn--subtab-option\"\r\n          @click=\"toggleFlipped\"\r\n        >\r\n          <span v-if=\"wantsFlipped\">Hide</span>\r\n          <span v-else>Show</span>\r\n          Hard V\r\n        </PrimaryButton>\r\n        <PrimaryButton\r\n          class=\"o-primary-btn--subtab-option l-cursed-glyph-creation\"\r\n          @click=\"createCursedGlyph\"\r\n        >\r\n          Create a Cursed Glyph\r\n        </PrimaryButton>\r\n        <br>\r\n        Cursed Glyphs can be created here or in the Effarig tab.\r\n        <br>\r\n        Cursed Glyphs count as {{ formatInt(-3) }} Glyphs for the purposes of all requirements related to Glyph count.\r\n        <br>\r\n        <span v-if=\"!isDoomed\">The Black Hole can now be used to slow down time if they are both permanent.</span>\r\n        <br><br>\r\n        Each Hard V-Achievement counts as two V-Achievements and will award {{ formatInt(2) }} Space Theorems\r\n        instead of {{ formatInt(1) }}.\r\n        <br>\r\n        Goal reduction is significantly more expensive for Hard V-Achievements.\r\n      </div>\r\n      <div\r\n        v-if=\"showReduction\"\r\n        class=\"c-v-info-text\"\r\n      >\r\n        You have {{ quantify(\"Perk Point\", pp, 2, 0) }}.\r\n      </div>\r\n      <div class=\"l-v-unlocks-container\">\r\n        <li\r\n          v-for=\"(hex, hexId) in hexGrid\"\r\n          :key=\"hexId + '-v-hex'\"\r\n          :style=\"[hex.isRunButton ? {zIndex: 1} : {zIndex: 0}]\"\r\n        >\r\n          <div\r\n            v-if=\"hex.config\"\r\n            class=\"l-v-hexagon c-v-unlock\"\r\n            :style=\"'background-color: ' + hexColor(hex)\"\r\n          >\r\n            <p class=\"o-v-unlock-name\">\r\n              <br v-if=\"hex.canBeReduced && showReduction\">{{ hex.config.name }}\r\n            </p>\r\n            <p\r\n              class=\"o-v-unlock-desc\"\r\n              v-html=\"hex.formattedDescription\"\r\n            />\r\n            <p\r\n              v-if=\"has(runMilestones[0][0]) && hex.isReduced\"\r\n              class=\"o-v-unlock-goal-reduction\"\r\n            >\r\n              Goal has been {{ mode(hex) }} by {{ reductionValue(hex) }}\r\n            </p>\r\n            <p class=\"o-v-unlock-amount\">\r\n              {{ formatInt(hex.completions) }}/{{ formatInt(hex.config.values.length) }} done\r\n            </p>\r\n            <div v-if=\"showRecord(hex)\">\r\n              <p class=\"o-v-unlock-record\">\r\n                Best: {{ hex.config.formatRecord(runRecords[hex.id]) }}\r\n              </p>\r\n              <p>\r\n                <GlyphSetPreview\r\n                  :glyphs=\"runGlyphs[hex.id]\"\r\n                  :text=\"hex.config.name\"\r\n                  :text-hidden=\"true\"\r\n                />\r\n              </p>\r\n              <div v-if=\"hex.canBeReduced && showReduction\">\r\n                <div class=\"l-v-goal-reduction-spacer\" />\r\n                <button\r\n                  class=\"o-primary-btn l-v-reduction\"\r\n                  :class=\"{ 'o-primary-btn--disabled': !hex.canBeReduced || pp < hex.reductionCost }\"\r\n                  :ach-tooltip=\"reductionTooltip(hex)\"\r\n                  @click=\"reduceGoals(hex)\"\r\n                >\r\n                  <i class=\"fas fa-angle-double-down\" />\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div\r\n            v-else-if=\"hex.isRunButton\"\r\n            :class=\"runButtonClassObject\"\r\n            @click=\"startRun()\"\r\n          >\r\n            <b\r\n              class=\"o-v-start-text\"\r\n              :class=\"{ 'o-pelle-disabled': isDoomed }\"\r\n            >\r\n              <span v-if=\"isRunning\">You are in </span>\r\n              <span v-else>Start </span>\r\n              V's Reality.\r\n            </b>\r\n            <br>\r\n            <div :style=\"{ 'font-size': hasAlchemy ? '1.2rem' : '' }\">\r\n              {{ runDescription }}\r\n            </div>\r\n            <div class=\"c-v-run-button__line c-v-run-button__line--1\" />\r\n            <div class=\"c-v-run-button__line c-v-run-button__line--2\" />\r\n            <div class=\"c-v-run-button__line c-v-run-button__line--3\" />\r\n          </div>\r\n          <div v-else>\r\n            <div class=\"l-v-hexagon l-placeholder-invisible\" />\r\n          </div>\r\n        </li>\r\n      </div>\r\n      <div class=\"c-v-info-text\">\r\n        V-Achievements can only be completed within V's Reality, but are permanent and do not reset upon leaving\r\n        and re-entering the Reality.\r\n      </div>\r\n      <div class=\"c-v-info-text\">\r\n        You have {{ formatInt(totalUnlocks) }} V-Achievements done.\r\n        <span v-if=\"!isDoomed\">\r\n          You gain {{ formatInt(1) }} Space Theorem for each completion,\r\n          allowing you to purchase Time Studies which are normally locked.\r\n          <br>\r\n          Space Theorems can also be used as a Currency in the Automator.\r\n        </span>\r\n      </div>\r\n      <br>\r\n      <div class=\"l-v-milestones-grid\">\r\n        <div\r\n          v-for=\"(row, rowId) in runMilestones\"\r\n          :key=\"rowId + '-v-ms-row'\"\r\n          class=\"l-v-milestones-grid__row\"\r\n        >\r\n          <div\r\n            v-for=\"(milestone, colId) in row\"\r\n            :key=\"colId + rowId*10 + '-v-ms'\"\r\n            class=\"o-v-milestone\"\r\n            :class=\"{'o-v-milestone--unlocked':\r\n              has(milestone)}\"\r\n          >\r\n            <div :class=\"{ 'o-pelle-disabled': isDoomed }\">\r\n              <p>{{ milestone.description }}</p>\r\n              <p>Reward: {{ milestone.rewardText }}</p>\r\n              <p v-if=\"milestone.formattedEffect\">\r\n                Currently: <b>{{ milestone.formattedEffect }}</b>\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.o-v-start-text {\r\n  font-size: 1.5rem;\r\n}\r\n\r\n.l-placeholder-invisible {\r\n  opacity: 0;\r\n}\r\n\r\n.l-v-goal-reduction-spacer {\r\n  height: 0.8rem;\r\n}\r\n\r\n.l-cursed-glyph-creation {\r\n  background: var(--color-effarig--base);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/tabs/celestial-v"},"metadata":{},"sourceType":"module"}