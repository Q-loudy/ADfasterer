{"ast":null,"code":"import AutobuyerIntervalLabel from \"./AutobuyerIntervalLabel\";\nimport AutobuyerModeButton from \"./AutobuyerModeButton\";\nimport AutobuyerSingleToggleLabel from \"./AutobuyerSingleToggleLabel\"; // This component contains individual autobuyers within groups of related autobuyers (eg. an individual AD autobuyer\n// within the all-ADs-in-a-row layout)\n\nexport default {\n  name: \"SingleAutobuyerInRow\",\n  components: {\n    AutobuyerSingleToggleLabel,\n    AutobuyerIntervalLabel,\n    AutobuyerModeButton\n  },\n  props: {\n    autobuyer: {\n      type: Object,\n      required: true\n    },\n    showIndividual: Boolean,\n    parentDisabled: Boolean\n  },\n\n  data() {\n    return {\n      isVisible: false\n    };\n  },\n\n  computed: {\n    name() {\n      return this.autobuyer.name;\n    },\n\n    hasMode() {\n      return this.autobuyer.mode !== undefined;\n    }\n\n  },\n  methods: {\n    update() {\n      const buyer = this.autobuyer;\n      this.isVisible = buyer.isUnlocked || buyer.isBought;\n    }\n\n  }\n};","map":{"version":3,"sources":["SingleAutobuyerInRow.vue"],"names":[],"mappings":"AACA,OAAA,sBAAA,MAAA,0BAAA;AACA,OAAA,mBAAA,MAAA,uBAAA;AACA,OAAA,0BAAA,MAAA,8BAAA,C,CAEA;AACA;;AACA,eAAA;AACA,EAAA,IAAA,EAAA,sBADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,0BADA;AAEA,IAAA,sBAFA;AAGA,IAAA;AAHA,GAFA;AAOA,EAAA,KAAA,EAAA;AACA,IAAA,SAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA,KADA;AAKA,IAAA,cAAA,EAAA,OALA;AAMA,IAAA,cAAA,EAAA;AANA,GAPA;;AAeA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,SAAA,EAAA;AADA,KAAA;AAGA,GAnBA;;AAoBA,EAAA,QAAA,EAAA;AACA,IAAA,IAAA,GAAA;AACA,aAAA,KAAA,SAAA,CAAA,IAAA;AACA,KAHA;;AAIA,IAAA,OAAA,GAAA;AACA,aAAA,KAAA,SAAA,CAAA,IAAA,KAAA,SAAA;AACA;;AANA,GApBA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,GAAA;AACA,YAAA,KAAA,GAAA,KAAA,SAAA;AACA,WAAA,SAAA,GAAA,KAAA,CAAA,UAAA,IAAA,KAAA,CAAA,QAAA;AACA;;AAJA;AA5BA,CAAA","sourcesContent":["<script>\r\nimport AutobuyerIntervalLabel from \"./AutobuyerIntervalLabel\";\r\nimport AutobuyerModeButton from