{"ast":null,"code":"import BlobBackground from \"@/components/BlobBackground\";\nimport BlobSnowflake from \"@/components/BlobSnowflake\";\nexport default {\n  name: \"BlobSnowflakes\",\n  components: {\n    BlobSnowflake,\n    BlobBackground\n  },\n\n  data() {\n    return {\n      animateBackground: false,\n      count: 0,\n      initialized: false,\n      bounds: {\n        x: 0,\n        y: 0\n      }\n    };\n  },\n\n  mounted() {\n    this.updateSize();\n    window.addEventListener(\"resize\", this.updateSize);\n    this.initialized = true;\n  },\n\n  destroyed() {\n    window.removeEventListener(\"resize\", this.updateSize);\n  },\n\n  methods: {\n    update() {\n      this.animateBackground = player.options.animations.background;\n      this.count = player.options.animations.blobSnowflakes;\n    },\n\n    updateSize() {\n      this.bounds.x = document.documentElement.clientWidth;\n      this.bounds.y = document.documentElement.clientHeight;\n    }\n\n  }\n};","map":{"version":3,"sources":["BlobSnowflakes.vue"],"names":[],"mappings":"AACA,OAAA,cAAA,MAAA,6BAAA;AACA,OAAA,aAAA,MAAA,4BAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,gBADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,aADA;AAEA,IAAA;AAFA,GAFA;;AAMA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,iBAAA,EAAA,KADA;AAEA,MAAA,KAAA,EAAA,CAFA;AAGA,MAAA,WAAA,EAAA,KAHA;AAIA,MAAA,MAAA,EAAA;AACA,QAAA,CAAA,EAAA,CADA;AAEA,QAAA,CAAA,EAAA;AAFA;AAJA,KAAA;AASA,GAhBA;;AAiBA,EAAA,OAAA,GAAA;AACA,SAAA,UAAA;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,QAAA,EAAA,KAAA,UAAA;AACA,SAAA,WAAA,GAAA,IAAA;AACA,GArBA;;AAsBA,EAAA,SAAA,GAAA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,UAAA;AACA,GAxBA;;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,GAAA;AACA,WAAA,iBAAA,GAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA,UAAA;AACA,WAAA,KAAA,GAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA,cAAA;AACA,KAJA;;AAKA,IAAA,UAAA,GAAA;AACA,WAAA,MAAA,CAAA,CAAA,GAAA,QAAA,CAAA,eAAA,CAAA,WAAA;AACA,WAAA,MAAA,CAAA,CAAA,GAAA,QAAA,CAAA,eAAA,CAAA,YAAA;AACA;;AARA;AAzBA,CAAA","sourcesContent":["<script>\r\nimport BlobBackground from \"@/components/BlobBackground\";\r\nimport BlobSnowflake from \"@/components/BlobSnowflake\";\r\n\r\nexport default {\r\n  name: \"BlobSnowflakes\",\r\n  components: {\r\n    BlobSnowflake,\r\n    BlobBackground,\r\n  },\r\n  data() {\r\n    return {\r\n      animateBackground: false,\r\n      count: 0,\r\n      initialized: false,\r\n      bounds: {\r\n        x: 0,\r\n        y: 0\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.updateSize();\r\n    window.addEventListener(\"resize\", this.updateSize);\r\n    this.initialized = true;\r\n  },\r\n  destroyed() {\r\n    window.removeEventListener(\"resize\", this.updateSize);\r\n  },\r\n  methods: {\r\n    update() {\r\n      this.animateBackground = player.options.animations.background;\r\n      this.count = player.options.animations.blobSnowflakes;\r\n    },\r\n    updateSize() {\r\n      this.bounds.x = document.documentElement.clientWidth;\r\n      this.bounds.y = document.documentElement.clientHeight;\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div v-if=\"initialized\">\r\n    <svg\r\n      v-if=\"animateBackground\"\r\n      class=\"c-blob-snowflake-container\"\r\n    >\r\n      <BlobSnowflake\r\n        v-for=\"i in count\"\r\n        :key=\"i\"\r\n        :bounds=\"bounds\"\r\n      />\r\n    </svg>\r\n    <svg\r\n      v-else\r\n      class=\"c-blob-background-container\"\r\n    >\r\n      <BlobBackground\r\n        v-for=\"i in count\"\r\n        :key=\"i\"\r\n        :bounds=\"bounds\"\r\n      />\r\n    </svg>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.c-blob-snowflake-container {\r\n  overflow: visible;\r\n  width: 100%;\r\n  height: 100%;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: 9999;\r\n  pointer-events: none;\r\n  -webkit-user-select: none;\r\n  user-select: none;\r\n  cursor: default;\r\n}\r\n\r\n.c-blob-background-container {\r\n  overflow: visible;\r\n  width: 100%;\r\n  height: 100%;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -9999;\r