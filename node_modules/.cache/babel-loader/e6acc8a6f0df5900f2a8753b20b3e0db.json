{"ast":null,"code":"import ExpandingControlBox from \"@/components/ExpandingControlBox\";\nexport default {\n  name: \"RemnantGainFactor\",\n  components: {\n    ExpandingControlBox\n  },\n  props: {\n    hide: {\n      type: Boolean,\n      required: false,\n      default: false\n    }\n  },\n\n  data() {\n    return {\n      best: {\n        am: new Decimal(0),\n        ip: new Decimal(0),\n        ep: new Decimal(0)\n      },\n      dilationMult: [1, 1, 1],\n      remnants: 0,\n      remnantsGain: 0\n    };\n  },\n\n  computed: {\n    opacity() {\n      return Number(!this.hide);\n    }\n\n  },\n  methods: {\n    update() {\n      this.best.am.copyFrom(player.celestials.pelle.records.totalAntimatter);\n      this.best.ip.copyFrom(player.celestials.pelle.records.totalInfinityPoints);\n      this.best.ep.copyFrom(player.celestials.pelle.records.totalEternityPoints);\n      this.dilationMult = PelleStrikes.dilation.hasStrike ? [500, 10, 5] : [1, 1, 1];\n      this.remnants = Pelle.cel.remnants;\n      this.remnantsGain = Pelle.remnantsGain;\n    }\n\n  }\n};","map":{"version":3,"sources":["RemnantGainFactor.vue"],"names":[],"mappings":"AACA,OAAA,mBAAA,MAAA,kCAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,mBADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA;AADA,GAFA;AAKA,EAAA,KAAA,EAAA;AACA,IAAA,IAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,QAAA,EAAA,KAFA;AAGA,MAAA,OAAA,EAAA;AAHA;AADA,GALA;;AAYA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,IAAA,EAAA;AACA,QAAA,EAAA,EAAA,IAAA,OAAA,CAAA,CAAA,CADA;AAEA,QAAA,EAAA,EAAA,IAAA,OAAA,CAAA,CAAA,CAFA;AAGA,QAAA,EAAA,EAAA,IAAA,OAAA,CAAA,CAAA;AAHA,OADA;AAMA,MAAA,YAAA,EAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CANA;AAOA,MAAA,QAAA,EAAA,CAPA;AAQA,MAAA,YAAA,EAAA;AARA,KAAA;AAUA,GAvBA;;AAwBA,EAAA,QAAA,EAAA;AACA,IAAA,OAAA,GAAA;AACA,aAAA,MAAA,CAAA,CAAA,KAAA,IAAA,CAAA;AACA;;AAHA,GAxBA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,GAAA;AACA,WAAA,IAAA,CAAA,EAAA,CAAA,QAAA,CAAA,MAAA,CAAA,UAAA,CAAA,KAAA,CAAA,OAAA,CAAA,eAAA;AACA,WAAA,IAAA,CAAA,EAAA,CAAA,QAAA,CAAA,MAAA,CAAA,UAAA,CAAA,KAAA,CAAA,OAAA,CAAA,mBAAA;AACA,WAAA,IAAA,CAAA,EAAA,CAAA,QAAA,CAAA,MAAA,CAAA,UAAA,CAAA,KAAA,CAAA,OAAA,CAAA,mBAAA;AACA,WAAA,YAAA,GAAA,YAAA,CAAA,QAAA,CAAA,SAAA,GAAA,CAAA,GAAA,EAAA,EAAA,EAAA,CAAA,CAAA,GAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA;AACA,WAAA,QAAA,GAAA,KAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,YAAA,GAAA,KAAA,CAAA,YAAA;AACA;;AARA;AA7BA,CAAA","sourcesContent":["<script>\r\nimport ExpandingControlBox from \"@/components/ExpandingControlBox\";\r\n\r\nexport default {\r\n  name: \"RemnantGainFactor\",\r\n  components: {\r\n    ExpandingControlBox\r\n  },\r\n  props: {\r\n    hide: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      best: {\r\n        am: new Decimal(0),\r\n        ip: new Decimal(0),\r\n        ep: new Decimal(0)\r\n      },\r\n      dilationMult: [1, 1, 1],\r\n      remnants: 0,\r\n      remnantsGain: 0\r\n    };\r\n  },\r\n  computed: {\r\n    opacity() {\r\n      return Number(!this.hide);\r\n    }\r\n  },\r\n  methods: {\r\n    update() {\r\n      this.best.am.copyFrom(player.celestials.pelle.records.totalAntimatter);\r\n      this.best.ip.copyFrom(player.celestials.pelle.records.totalInfinityPoints);\r\n      this.best.ep.copyFrom(player.celestials.pelle.records.totalEternityPoints);\r\n      this.dilationMult = PelleStrikes.dilation.hasStrike ? [500, 10, 5] : [1, 1, 1];\r\n      this.remnants = Pelle.cel.remnants;\r\n      this.remnantsGain = Pelle.remnantsGain;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div class=\"c-remnant-factors-container\">\r\n    <ExpandingControlBox\r\n      container-class=\"c-remnant-factors\"\r\n      label=\"Remnant Gain Factors\"\r\n      :style=\"{ opacity }\"\r\n    >\r\n      <template #dropdown>\r\n        <div class=\"c-remnant-factors-text\">\r\n          Best AM: {{ format(best.am, 2, 2) }}<br>\r\n          Best IP: {{ format(best.ip, 2, 2) }}<br>\r\n          Best EP: {{ format(best.ep, 2, 2) }}<br><br>\r\n          <div class=\"l-remnant-factors-row\">\r\n            <div class=\"l-remnant-factors-col l-remnant-factors-col--first\">\r\n              <div class=\"l-remnant-factors-item\">\r\n                log10(log10(am){{ dilationMult[0] > 1 ? `*${dilationMult[0]}` : \"\" }} + 2)\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                log10(log10(ip){{ dilationMult[1] > 1 ? `*${dilationMult[1]}` : \"\" }} + 2)\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                log10(log10(ep){{ dilationMult[2] > 1 ? `*${dilationMult[2]}` : \"\" }} + 2)\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                Static divisor\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                Static power\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                Existing Remnants\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                Final amount\r\n              </div>\r\n            </div>\r\n            <div class=\"l-remnant-factors-col\">\r\n              <div class=\"l-remnant-factors-item\" />\r\n              <div class=\"l-remnant-factors-item\">\r\n                +\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                +\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                /\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                ^\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                -\r\n              </div>\r\n            </div>\r\n            <div class=\"l-remnant-factors-col\">\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(Math.log10(best.am.add(1).log10()*dilationMult[0] + 2), 2, 2) }}\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(Math.log10(best.ip.add(1).log10()*dilationMult[0] + 2), 2, 2) }}\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(Math.log10(best.ep.add(1).log10()*dilationMult[0] + 2), 2, 2) }}\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(1.64, 2, 2) }}\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(7.5, 2, 2) }}\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(remnants, 2, 0) }}\r\n              </div>\r\n              <div class=\"l-remnant-factors-item\">\r\n                {{ format(remnantsGain, 2, remnantsGain >= 1 ? 0 : 2) }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </template>\r\n    </ExpandingControlBox>\r\n  </div>\r\n</template>\r\n\r\n<style>\r\n.c-remnant-factors-container {\r\n  position: relative;\r\n  z-index: 4;\r\n}\r\n\r\n.c-remnant-factors-container .l-expanding-control-box {\r\n  transition: opacity 0.5s;\r\n}\r\n\r\n.c-remnant-factors {\r\n  z-index: 4;\r\n  font-weight: bold;\r\n  color: var(--color-text);\r\n  background-color: var(--color-text-inverted);\r\n  border: var(--var-border-width, 0.2rem) solid var(--color-pelle--base);\r\n  border-radius: var(--var-border-radius, 0.5rem);\r\n  padding: 0.3rem;\r\n}\r\n\r\n.c-remnant-factors-text {\r\n  padding: 0.3rem;\r\n}\r\n\r\n.l-remnant-factors-row {\r\n  display: flex;\r\n  width: 100%;\r\n}\r\n\r\n.l-remnant-factors-col {\r\n  display: flex;\r\n  flex-grow: 0;\r\n  flex-shrink: 0;\r\n  flex-direction: column;\r\n  margin-left: 2rem;\r\n}\r\n\r\n.l-remnant-factors-col--first {\r\n  flex-grow: 1;\r\n  margin-left: 0;\r\n}\r\n\r\n.l-remnant-factors-item {\r\n  height: 2rem;\r\n  text-align: left;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/tabs/celestial-pelle"},"metadata":{},"sourceType":"module"}