r\n    >\r\n      <div\r\n        class=\"c-autobuyer-box__small-text\"\r\n      >\r\n        <br>\r\n        Activates every X seconds:\r\n      </div>\r\n      <AutobuyerInput\r\n        :autobuyer=\"autobuyer\"\r\n        type=\"float\"\r\n        property=\"buyMaxInterval\"\r\n      />\r\n    </template>\r\n    <template\r\n      v-if=\"!isBuyMaxUnlocked\"\r\n      #checkboxSlot\r\n    >\r\n      <label\r\n        class=\"o-autobuyer-toggle-checkbox c-autobuyer-box__small-text l-top-margin o-clickable\"\r\n      >\r\n        <input\r\n          v-model=\"limitDimBoosts\"\r\n          type=\"checkbox\"\r\n          class=\"o-clickable\"\r\n        >\r\n        Limit Dimension Boosts to:\r\n      </label>\r\n      <AutobuyerInput\r\n        :autobuyer=\"autobuyer\"\r\n        type=\"int\"\r\n        property=\"maxDimBoosts\"\r\n      />\r\n    </template>\r\n    <template #toggleSlot>\r\n      <label\r\n        class=\"o-autobuyer-toggle-checkbox c-autobuyer-box__small-text l-autobuyer-text-area o-clickable\"\r\n      >\r\n        <input\r\n          v-model=\"limitUntilGalaxies\"\r\n          type=\"checkbox\"\r\n          class=\"o-clickable\"\r\n        >\r\n        <span v-if=\"isBuyMaxUnlocked\">\r\n          Only Dimboost to unlock new<br>\r\n          Dimensions until X Galaxies:\r\n        </span>\r\n        <span v-else>\r\n          Galaxies required to always<br>\r\n          Dimboost, ignoring the limit:\r\n        </span>\r\n      </label>\r\n      <AutobuyerInput\r\n        :autobuyer=\"autobuyer\"\r\n        type=\"int\"\r\n        property=\"galaxies\"\r\n      />\r\n    </template>\r\n  </AutobuyerBox>\r\n</template>\r\n\r\n<style scoped>\r\n.l-top-margin {\r\n  margin-top: 0.82rem;\r\n}\r\n\r\n.l-dimboost-text-area {\r\n  height: 3rem;\r\n}\r\n\r\n.o-clickable {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/tabs/autobuyers"},"metadata":{},"sourceType":"module"}